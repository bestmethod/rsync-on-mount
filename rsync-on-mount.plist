<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bestmethod.rsync-on-mount</string>
    <key>ProgramArguments</key>
    <array>
        <string>touch</string>
        <string>/tmp/volumes-changed</string>
    </array>
    <key>WatchPaths</key>
      <array>
         <string>/Volumes/</string>
      </array>
   <key>KeepAlive</key>
      <false/>
</dict>
</plist>
